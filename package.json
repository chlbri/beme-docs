{
  "name": "@bemedev/docs",
  "version": "0.0.5",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "dev": "vite dev",
    "dev:host": "pnpm run dev --host",
    "build": "vite build",
    "preview": "pnpm run build && vite start",
    "ci": "pnpm run config:off && pnpm run lint && pnpm run test && pnpm run p-q",
    "ci:admin": "pnpm run rm && pnpm run upgrade && pnpm run ci",
    "posttest": "pnpm run lint",
    "pretest": "pnpm run build",
    "prettier": "prettier --write \"src/**/*.{ts,tsx}\" --log-level silent",
    "rinit": "pnpm run rm && pnpm run config",
    "rinit:off": "pnpm run rm && pnpm run config:off",
    "rm": "pnpx rimraf ./node_modules ./pnpm-lock.yaml",
    "rollup": "rollup -c",
    "test": "pnpm run test:coverage --no-coverage",
    "test:only": "pnpm run test:coverage --no-coverage",
    "test:watch": "vitest",
    "test:coverage": "vitest run",
    "config": "pnpm install",
    "config:off": "pnpm run config --offline",
    "eslint": "eslint \"{src/**/*,App}.{ts,tsx}\"",
    "lint": "pnpm run prettier && pnpm run eslint --fix",
    "lint:watch": "onchange --await-write-finish 5500 -d 50000 -v \"**/*.ts\" -- pnpm run lint",
    "p-q": "pretty-quick",
    "start": "vite start",
    "upgrade": "pnpm up --latest",
    "prepare": "husky"
  },
  "dependencies": {
    "@bemedev/app-ts": "^1.6.8",
    "@bemedev/decompose": "^1.5.0",
    "@bemedev/fsf": "^1.0.1",
    "@bemedev/i18n": "^0.5.2",
    "@bemedev/pipe": "^0.1.0",
    "@bemedev/sleep": "^0.1.2",
    "@kobalte/core": "^0.13.11",
    "@solid-primitives/intersection-observer": "^2.2.2",
    "@solid-primitives/storage": "^4.3.3",
    "@tailwindcss/vite": "^4.1.18",
    "@tanstack/pacer-lite": "^0.2.1",
    "@tanstack/solid-pacer": "^0.18.1",
    "@tanstack/solid-router": "1.147.3",
    "@tanstack/solid-start": "1.149.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "color": "^5.0.3",
    "lucide-solid": "^0.562.0",
    "nitro": "3.0.1-alpha.1",
    "redaxios": "^0.5.1",
    "solid-js": "^1.9.10",
    "solid-motionone": "^1.0.4",
    "tailwind-merge": "^3.4.0",
    "tailwindcss": "^4.1.18",
    "tailwindcss-animate": "^1.0.7",
    "tw-animate-css": "^1.4.0",
    "valibot": "^1.2.0",
    "vinxi": "0.5.10"
  },
  "devDependencies": {
    "@bemedev/app-cli": "^0.1.7",
    "@bemedev/build-tests": "^0.1.2",
    "@bemedev/rollup-config": "^0.2.2",
    "@bemedev/vitest-alias": "^0.0.3",
    "@bemedev/vitest-exclude": "^0.1.1",
    "@bemedev/vitest-extended": "^1.5.3",
    "@content-collections/core": "^0.13.1",
    "@content-collections/markdown": "^0.1.4",
    "@content-collections/vite": "^0.2.8",
    "@eslint/eslintrc": "^3.3.3",
    "@eslint/js": "^9.39.2",
    "@solidjs/testing-library": "^0.8.10",
    "@tanstack/nitro-v2-vite-plugin": "1.141.0",
    "@tanstack/solid-router-devtools": "1.149.0",
    "@toolwind/corner-shape": "0.0.8-3",
    "@types/clone-deep": "^4.0.4",
    "@types/node": "^25.0.7",
    "@typescript-eslint/eslint-plugin": "^8.53.0",
    "@typescript-eslint/parser": "^8.53.0",
    "@vitest/browser-playwright": "4.0.17",
    "@vitest/coverage-istanbul": "^4.0.17",
    "combinate": "^1.1.11",
    "eslint": "^9.39.2",
    "glob": "^13.0.0",
    "globals": "^17.0.0",
    "husky": "^9.1.7",
    "nitro": "3.0.1-alpha.1",
    "onchange": "^7.1.0",
    "playwright": "^1.57.0",
    "prettier": "^3.7.4",
    "prettier-plugin-tailwindcss": "^0.7.2",
    "pretty-quick": "^4.2.2",
    "rimraf": "^6.1.2",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "vite": "npm:rolldown-vite@^7.3.1",
    "vite-plugin-solid": "^2.11.10",
    "vite-tsconfig-paths": "^6.0.4",
    "vitest": "4.0.17"
  },
  "overrides": {
    "vite": "npm:rolldown-vite@latest"
  }
}
